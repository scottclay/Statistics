    "plt.ylabel('Cumulative Probability',fontsize=14)\n",
    "plt.title('Cumulative Distribution Function',fontsize=18)\n",
    "plt.plot(energies,cum_prob,linewidth=3)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def random_sample(N):\n",
    "    R = np.random.uniform(0, 1, N)\n",
    "    gen_energies = [int(energies[np.argwhere(cum_prob == min(cum_prob[(cum_prob - r) > 0]))]) for r in R]\n",
    "    times = np.arange(0, 9, 1)\n",
    "    lc = np.bincount(gen_energies, minlength=len(times))\n",
    "    lc_prob = lc/sum(lc)\n",
    "    return lc_prob"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "N_100 = random_sample(100)\n",
    "N_1000 = random_sample(1000)\n",
    "N_10000 = random_sample(10000)"
   ]
  },
  {
   "cell_type": "code",
